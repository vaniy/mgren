    <script>
        $(function () {
            var addressInfo = {};
            $('#demo').citys({
                required: false,
                nodata: 'disabled',
                onChange: function (data) {
                    addressInfo = {
                        province: data.province,
                        // city: data.city,
                        // area: data.area,
                    };
                }
            });
            $('.addPropertyDom').click((function (e) {
                var properyDom = $('.propertyDom').clone();
                properyDom.find('.addPropertyDom').remove();
                $($('.propertyDom')[0]).after($(properyDom[0]))
            }))

            $('.btn.btn-primary').click(function (e) {
                e.preventDefault();

                var data = new FormData();

                var integratorId = $('input.integratorId').val();
                data.append("integratorId", integratorId);
                var integratorName = $('input.integratorName').val();
                data.append("integratorName", integratorName);
                var integratorNationCity = $('input.integratorNationCity').val();
                data.append("integratorNationCity", integratorNationCity);
                var cover = $('input.cover').val();
                data.append("cover", cover);
                var content = $('input.content').val();
                data.append("content", content);
                var address = $('input.address').val();
                data.append("address", address);
                var phone = $('input.phone').val();
                data.append("phone", phone);
                var fax = $('input.fax').val();
                data.append("fax", fax);
                var email = $('input.email').val();
                data.append("email", email);
                var priority = $('input.priority').val();
                data.append("priority", priority)
                if (addressInfo.province && integratorNationCity.trim == "") {
                    data.append("province", addressInfo.province);
                }

                if (document.getElementsByClassName('integratorImg')[0].files[0]) {
                    data.append("integratorImg", document.getElementsByClassName('integratorImg')[0].files[0])
                }
                $.ajax({
                    url: integratorId && integratorId.trim() != '' ? '/api/updateIntegrator?iid=' + integratorId.trim() : '/api/createIntegrator',
                    type: 'POST',
                    data: data,
                    async: false,
                    cache: false,
                    contentType: false,
                    processData: false,
                    success: function (data) {
                        console.log(data);
                        if (data.status && data.status === 'success') {
                            alert(data.msg)
                            location.href = '/management/integratorMgrM'
                        } else if (data.msg) {
                            alert(data.msg)
                            // console.log(data.message);
                        }
                    },
                    error: function (data) {
                        console.log(data.status);
                    }
                });
            })
        })
    </script>
