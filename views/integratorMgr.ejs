                            <div class="card-header">
                                <h3 class="card-title">
                                    <%=title %>
                                </h3>
                            </div>
                            <!-- /.card-header -->
                            <!-- form start -->
                            <form role="form">
                                <div class="card-body">
                                    <input class="integratorId" type="text" hidden value="<%=integrator.iid || '' %>">
                                    <div class="form-group">
                                        <label for="name">集成商名称</label>
                                        <input value="<%=integrator.integratorName ? integrator.integratorName : '' %>"
                                            class="form-control integratorName" type="text" placeholder="例如：上海亨路网络科技有限公司">
                                    </div>

                                    <div class="form-group">
                                        <label for="name">集成商排序</label>
                                        <input value="<%=integrator.priority %>" class="form-control priority" type="text"
                                            placeholder="例如：1">
                                    </div>

                                    <div class="form-group">
                                        <label for="name">所在城市(只有国内集成商需要选择)
                                            <%if(integrator.province && integrator.province.length > 0){%>
                                            (
                                            <%= integrator.province%>)
                                            <%} %>
                                        </label>
                                        <div class="citys">
                                            <div id="demo">
                                                <select name="province" class="form-control"></select>
                                                <!-- <select name="city" class="form-control"></select>
                                                <select name="area" class="form-control"></select> -->
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="name">所在国际城市（只有国际集成商需要填写）</label>
                                        <input value="<%=integrator.integratorNationCity ? integrator.integratorNationCity : '' %>"
                                            class="form-control integratorNationCity" type="text" placeholder="例如：Los Angeles">
                                    </div>

                                    <div class="
                                            form-group">
                                        <label for="name">服务区域</label>
                                        <input value="<%=integrator.cover ? integrator.cover : '' %>" class="form-control cover"
                                            type="text" placeholder="例如：上海市长宁区">
                                    </div>
                                    <div class="form-group">
                                        <label for="name">服务内容</label>
                                        <input value="<%=integrator.content ? integrator.content : '' %>" class="form-control content"
                                            type="text" placeholder="例如：系统集成，安装，保养">
                                    </div>
                                    <div class="form-group">
                                        <label for="name">地址</label>
                                        <input value="<%=integrator.address ? integrator.address : '' %>" class="form-control address"
                                            type="text" placeholder="例如：上海市闵行区吴中路1079号405">
                                    </div>
                                    <div class="form-group">
                                        <label for="name">电话</label>
                                        <input value="<%=integrator.phone ? integrator.phone : '' %>" class="form-control phone"
                                            type="text" placeholder="例如：021-6234 2323">
                                    </div>
                                    <div class="form-group">
                                        <label for="name">传真</label>
                                        <input value="<%=integrator.fax ? integrator.fax : '' %>" class="form-control fax"
                                            type="text" placeholder="例如：021-6234 2324">
                                    </div>
                                    <div class="form-group">
                                        <label for="name">email</label>
                                        <input value="<%=integrator.email ? integrator.email : '' %>" class="form-control email"
                                            type="text" placeholder="例如：info@592hl.com">
                                    </div>

                                    <div class="form-group">
                                        <label for="exampleInputFile">集成商图片</label>
                                        <div class="input-group" data-provides="fileinput">
                                            <div class="custom-file">
                                                <input type="file" class="custom-file-input integratorImg" id="exampleInputFile"
                                                    accept="image/gif,image/jpeg,image/x-png">
                                                <label class="custom-file-label" for="exampleInputFile">选择图片</label>
                                            </div>
                                            <div class="input-group-append">
                                                <!-- <span class="input-group-text" id="">Upload</span> -->
                                            </div>
                                            <div class="fileinput-preview fileinput-exists thumbnail" style="max-width: 200px; max-height: 150px;"></div>
                                            <% if(integrator.imgs && integrator.imgs.length >= 0){%>
                                            <img style="width: 200px;height:150px" src="<%=integrator.imgs %>" alt="">
                                            <%} %>
                                        </div>
                                    </div>

                                </div>
                                <!-- /.card-body -->

                                <div class="card-footer">
                                    <button type="submit" class="btn btn-primary">Submit</button>
                                </div>
                            </form>
                        </div>
                        <!-- /.card -->

                    </div>
                </div><!-- /.container-fluid -->
