    <script>
        $(function () {

            $('.addBanner').click((function (e) {
                var html = '<div class="row bannerDom">' +
                    '<div class="col-11 input-group" data-provides="fileinput">' +
                    '<div class="custom-file">' +
                    '<input type="file" class="custom-file-input " id="exampleInputFile' + ' banner' + this.id + '"' +
                    'accept="image/gif,image/jpeg,image/x-png">' +
                    '<label class="custom-file-label" for="exampleInputFile">选择图片</label>' +
                    '</div>' +
                    '<div class="input-group-append"</div>' +
                    '<div class="fileinput-preview fileinput-exists thumbnail" style="max-width: 200px; max-height: 150px;">' +
                    '<img style="width: 200px;height:150px" src="" alt="">' +
                    '</div>' +
                    '</div>' +
                    '</div>' +
                    '<div class="col-1">' +
                    '<button type="button" class="btn btn-danger deleteBanner">X</button>' +
                    '</div>' +
                    '</div>'
                var bannerDom = $(html);
                var currentDom = $(this.parentElement);
                $(currentDom.find('.bannerDom')[0]).after($(bannerDom[0]));

                $('.deleteBanner').unbind('click');
                $('.deleteBanner').click(function (e) {
                    if ($(this.parentElement.parentElement.parentElement).find('.bannerDom').length == 1) {
                        alert('必须添加一个')
                    }
                    else {
                        this.parentElement.parentElement.remove()
                    }
                })
            }))

            $('.deleteBanner').click(function (e) {
                if ($(this.parentElement.parentElement.parentElement).find('.bannerDom').length == 1) {
                    alert('必须添加一个')
                }
                else {
                    this.parentElement.parentElement.remove()
                }
            })
            $('.btn.btn-primary').click(function (e) {
                e.preventDefault();

                var data = new FormData();
                var bannerId = this.id;
                var bannerDom = $(this.parentElement.parentElement).find('input.custom-file-input');
                var length = bannerDom.length;
                if (length <= 0) {
                    return;
                }
                data.append('id', this.id)
                var key = '';
                switch (bannerId) {
                    case '1':
                        key = 'banner_a_';
                        break;
                    case '2':
                        key = 'banner_b_';
                        break;
                    case '3':
                        key = 'banner_c_';
                        break;
                    case '4':
                        key = 'banner_d_';
                        break;
                    case '5':
                        key = 'banner_e_';
                        break;
                    case '6':
                        key = 'banner_f_';
                        break;
                }
                var banners = '';
                for (var i = 0; i < length; i++) {
                    debugger;
                    var img = $(this.parentElement.parentElement).find('img')[i];
                    if (img && img.src != '' && img.src.indexOf('/home/') >= 0) {
                        banners = banners == '' ? img.src : banners + ',' + img.src;
                    }
                    else {
                        data.append(key + i, $(this.parentElement.parentElement).find('input.custom-file-input')[i].files[0]);

                    }
                }
                if (banners != '') {
                    data.append('banners', banners)
                }

                $.ajax({
                    url: '/api/updateBanner',
                    type: 'POST',
                    data: data,
                    async: false,
                    cache: false,
                    contentType: false,
                    processData: false,
                    success: function (data) {
                        console.log(data);
                        if (data.status && data.status === 'success') {
                            alert(data.msg)
                            location.href = '/management/bannerMgr'
                        } else if (data.msg) {
                            alert(data.msg)
                            // console.log(data.message);
                        }
                    },
                    error: function (data) {
                        console.log(data.status);
                    }
                });
            })
        })
    </script>
