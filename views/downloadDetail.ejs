<script type="text/javascript">
//右侧悬浮
$(document).ready(function(){

	$(document).on("click", ".zi-fdTel .up", function(){
		$("html,body").animate({scrollTop: 0});
	});
	$(window).scroll(function(){
		var st = $(document).scrollTop();
		var $top = $(".zi-fdTel");
		if(st > 400){
			$top.css({display: 'block'});
		}else{
			if ($top.is(":visible")) {
				$top.hide();
			}
		}
	});
	
});	
 $(function () {
