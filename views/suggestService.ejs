<!-- Initialize Swiper -->
<script type="text/javascript">
	var qualityAssurance = 0;
	$(".fw-xz li").click(function () {
		$(".fw-xz li").removeClass("active");
		qualityAssurance = $(this).attr('data-target');
		$(this).addClass("active");
	});
	$(function () {
		//比较简洁，细节可自行完善
		$('.btn.btn-car.btn-car-sm').click(function (e) {
			e.preventDefault();
			var data = new FormData();
			// debugger
			// var customQuestion = '';
			// var oid = $('input.oid').val();
			// if (jQuery(".customQuestions  option:selected")) {
			// 	customQuestion = jQuery(".customQuestions  option:selected").text().trim();
			// 	data.append("customQuestion", customQuestion);
			// }
			if (document.getElementsByClassName('img')[0].files[0]) {
				data.append("img", document.getElementsByClassName('img')[0].files[0])
			}
			data.append("description", $('textarea.description').val())
			// data.append("oid", $('input.oid').val())
			data.append("title", $('input.title').val())

			$.ajax({
				url: '/api/createService?type=1',
				type: 'POST',
				data: data,
				async: false,
				cache: false,
				contentType: false,
				processData: false,
				success: function (data) {
					console.log(data);
					if (data.status && data.status == 'success') {
						alert('提交成功');
						location.href = "/suggsestServiceList"
					} else {
						alert(data.msg);
					}
				},
				error: function (data) {
					console.log(data.status);
				}
			});
		});

	})
	$(function () {
		var winWide = window.screen.width;

		var wideScreen = false;
