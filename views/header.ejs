<!-- Initialize Swiper -->
<script type="text/javascript">
    //右侧悬浮
    $(document).ready(function () {
        $.ajax({
            url: '/api/getUser',
            type: 'GET',
            async: false,
            cache: false,
            success: function (data) {
                console.log(data);
                if (data.status && data.status === 'success') {
                    $('.userName').text(data.content)
                    // alert(data.content)
                    // location.href = '/management/integratorMgrM'
                } else if (data.msg) {
                    alert(data.msg)
                    // console.log(data.message);
                }
            },
            error: function (data) {
                console.log(data.status);
            }
        });
    });
</script>

