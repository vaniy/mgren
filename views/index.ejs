<!-- Initialize Swiper -->
<script type="text/javascript">
	//右侧悬浮
	$(document).ready(function () {

		$(document).on("click", ".zi-fdTel .up", function () {
			$("html,body").animate({ scrollTop: 0 });
		});
		$(window).scroll(function () {
			var st = $(document).scrollTop();
			var $top = $(".zi-fdTel");
			if (st > 400) {
				$top.css({ display: 'block' });
			} else {
				if ($top.is(":visible")) {
					$top.hide();
				}
			}
		});

	});

	var aaa = new Swiper('.swiper-container01', {
		pagination: '.swiper-pagination',
		paginationClickable: true,
		speed: 1000,
		loop: true,
		nextButton: '.swiper-button-next01',
		prevButton: '.swiper-button-prev01',
		observer: true,
		observeParents: true,
		autoplayDisableOnInteraction: false,
		autoplay: 2000
	});
	$('.swiper-container01').mouseover(function () {
		aaa.stopAutoplay();
	})
	$('.swiper-container01').mouseout(function () {
		aaa.startAutoplay();
	})
	$(function () {
		if (screen.width >= 767) {
			var anli = new Swiper('.swiper-container02', {
				slidesPerView: 3,
				speed: 1000,
				autoplay: 2000,
				loop: true,
				observer: true,
				observeParents: true,
				paginationClickable: true,
				nextButton: '.swiper-button-next02',
				prevButton: '.swiper-button-prev02',
				spaceBetween: 5
			});
			$('.swiper-container02').mouseover(function () {
				anli.stopAutoplay();
			})
			$('.swiper-container02').mouseout(function () {
				anli.startAutoplay();
			})
			var huoban = new Swiper('.swiper-container03', {
				slidesPerView: 5,
				speed: 1000,
				autoplay: 2000,
				loop: true,
				observer: true,
				observeParents: true,
				paginationClickable: true,
				nextButton: '.swiper-button-next03',
				prevButton: '.swiper-button-prev03',
				spaceBetween: 30
			});
			$('.swiper-container03').mouseover(function () {
				huoban.stopAutoplay();
			})
			$('.swiper-container03').mouseout(function () {
				huoban.startAutoplay();
			})
		} else {
			var anli = new Swiper('.swiper-container02', {
				slidesPerView: 1,
				speed: 1000,
				autoplay: 2000,
				loop: true,
				observer: true,
				observeParents: true,
				paginationClickable: true,
				nextButton: '.swiper-button-next02',
				prevButton: '.swiper-button-prev02',
				spaceBetween: 5
			});
			$('.swiper-container02').mouseover(function () {
				anli.stopAutoplay();
			})
			$('.swiper-container02').mouseout(function () {
				anli.startAutoplay();
			})
			var huoban = new Swiper('.swiper-container03', {
				slidesPerView: 2,
				speed: 1000,
				autoplay: 2000,
				loop: true,
				observer: true,
				nextButton: '.swiper-button-next03',
				prevButton: '.swiper-button-prev03',
				observeParents: true,
				paginationClickable: true,
				paginationClickable: true,
				spaceBetween: 30
			});
			$('.swiper-container03').mouseover(function () {
				huoban.stopAutoplay();
			})
			$('.swiper-container03').mouseout(function () {
				huoban.startAutoplay();
			})
		}
	})
	$(".navbar-toggle").click(function () {
		$(".nav-down").slideUp("slow");
		if ($(".nav-list").is(":hidden")) {
			$(".nav-list").slideDown("slow");
		} else {
			$(".nav-list").slideUp("slow");
		}
	});
	$(".phone-i").click(function () {
		if ($(this).parents("li").find(".nav-down").is(":hidden")) {
			$(".nav-down").slideUp("slow");
			$(this).parents("li").find(".nav-down").slideDown("slow");
		} else {
			$(this).parents("li").find(".nav-down").slideUp("slow");
		}
	});
	$('.glyphicon.glyphicon-search').click(function (e) {
		e.preventDefault();
		var keywords = $('.keywords').val();
		if (!keywords || keywords.trim() == '') {
			return;
		}
		$.ajax({
			url: '/api/search?keywords=' + keywords,
			type: 'GET',
			async: false,
			cache: false,
			success: function (data) {
				console.log(data);
				if (data.status && data.status === 'success') {
					// $('.userName').text(data.content)
					// alert(data.content)
					if (data.msg && data.msg.length > 0) {
						location.href = '/productDetail?pid=' + data.msg
					}
					// location.href = '/management/integratorMgrM'
				} else if (data.msg) {
					alert(data.msg)
					// console.log(data.message);
				}
			},
			error: function (data) {
				console.log(data.status);
			}
		});
	})

</script>

